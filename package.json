{
  "name": "vue-next-infinite-scroll",
  "version": "0.0.3",
  "type": "module",
  "author": "ykx",
  "description": "基于vue3的滚动插件",
  "keywords": [
    "infinite-scroll",
    "seamless-scroll",
    "infinite",
    "seamless",
    "scroll"
  ],
  "files": [
    "lib"
  ],
  "main": "./lib/VueNextInfiniteScroll.umd.cjs",
  "module": "./lib/VueNextInfiniteScroll.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whateveryoudo/vue3-infinite-scroll"
  },
  "exports": {
    "./lib/style.css":"./lib/style.css",
    ".": {
      "import": "./lib/VueNextInfiniteScroll.js",
      "require": "./lib/VueNextInfiniteScroll.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs --port 3000",
    "docs:build": "vitepress build docs"
  },
  "dependencies": {
    "vitepress": "^1.0.0-alpha.19",
    "vue": "^3.2.37",
    "vue-next-infinite-scroll": "^0.0.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  },
  "license": "ISC"
}
